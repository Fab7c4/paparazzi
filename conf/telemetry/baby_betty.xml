<?xml version="1.0"?>

<!--NOTE, THE MAX FREQUENCY FOR A PACKAGE IS THE FEAUENCY AT WHICH THIS TELEMETRY MODULE IS CALLED IN THE CONF FILE-->
<!DOCTYPE telemetry SYSTEM "telemetry.dtd">
<telemetry>
  <process name="Main">
    <mode name="default" key_press="d">
	<message name="IMU_ACCEL_SCALED"	 period="0.02"/>
    	<message name="IMU_GYRO_SCALED"		 period="0.02"/>
	<message name="IMU_MAG_SCALED"		 period="0.02"/>
	<message name="ROTORCRAFT_RADIO_CONTROL" period="5"/>
	<message name="AHRS_QUAT_INT"   	 period="5"/>
	<message name="GPS_INT"			 period="5"/>
	<message name="UART_ERRORS"		 period="5"/>
	<message name="ACTUATORS"		 period="5"/>
    </mode>
  </process>
</telemetry>


<!--
OTHER MESSAGES DEFINED AS MODULES IN MAIN CONFIGURATION FILE
airspeed ets	10hz
sysmon			5hz
-->
